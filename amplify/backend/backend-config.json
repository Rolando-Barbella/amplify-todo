{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"providerPlugin": "awscloudformation",
			"type": "manual"
		}
	},
	"auth": {
		"amplifytodo": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"analytics": {
		"amplifytodoanalytics": {
			"service": "Pinpoint",
			"providerPlugin": "awscloudformation"
		}
	},
	"storage": {
		"amplifytodostorage": {
			"service": "S3",
			"providerPlugin": "awscloudformation"
		}
	},
	"api": {
		"amplifytodo": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authamplifytodo"
						}
					}
				}
			}
		}
	}
}